#login-modal {
    background-color: #ffffff;
    width: 300px;
    height: 200px;
    padding: 20px;
    margin: auto;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    border-radius: 10px;
    box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);
}

.header {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 10px;
}

TextField {
    margin-bottom: 10px;
}
